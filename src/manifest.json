{
  "manifest_version": 1,
  "version": 1,
  "name": {
    "en": "LINE Messaging API Plugin",
    "ja": "LINEメッセージ配信プラグイン"
  },
  "description": {
    "en": "send LINE message to customers",
    "ja": "顧客管理アプリに登録されている顧客にLINEメッセージを配信するプラグイン"
  },
  "type": "APP",
  "icon": "image/icon.png",
  "desktop": {
    "js": [
      "https://unpkg.com/@kintone/rest-api-client@latest/umd/KintoneRestAPIClient.min.js",
      "https://cdn.jsdelivr.net/npm/vue@2.6.14/dist/vue.min.js",
      "https://unpkg.com/element-ui/lib/index.js",
      "js/vue-message-modal.js",
      "js/desktop.js"
    ],
    "css": [
      "https://unpkg.com/element-ui/lib/theme-chalk/index.css",
      "css/51-modern-default.css",
      "css/desktop.css"
    ]
  },
  "mobile": {
    "js": [
      "https://js.cybozu.com/jquery/3.3.1/jquery.min.js",
      "js/mobile.js"
    ],
    "css": [
      "css/mobile.css"
    ]
  },
  "config": {
    "html": "html/config.html",
    "js": [
      "https://unpkg.com/@kintone/rest-api-client@latest/umd/KintoneRestAPIClient.min.js",
      "https://cdn.jsdelivr.net/npm/vue@2.6.14/dist/vue.min.js",
      "https://unpkg.com/element-ui/lib/index.js",
      "js/config.js"
    ],
    "css": [
      "https://unpkg.com/element-ui/lib/theme-chalk/index.css",
      "https://use.fontawesome.com/releases/v5.6.1/css/all.css",
      "https://cdn.jsdelivr.net/combine/npm/bootstrap@5/dist/css/bootstrap-grid.min.css,npm/bootstrap@5/dist/css/bootstrap-utilities.min.css",
      "css/51-modern-default.css",
      "css/kintone-ui-component.min.css",
      "css/config.css"
    ],
    "required_params": [
      "message"
    ]
  }
}